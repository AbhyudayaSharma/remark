{
  "name": "mdast",
  "version": "0.0.1",
  "description": "Speedy Markdown parser for multipurpose analysis",
  "author": "Christopher Jeffrey",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com>"
  ],
  "keywords": [
    "markdown",
    "markup",
    "abstract",
    "syntax",
    "tree",
    "json",
    "ast"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/mdast.git"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^0.7.4",
    "istanbul": "^0.3.0",
    "jscs": "^1.5.8",
    "markdown": "*",
    "matcha": "^0.5.0",
    "mocha": "^1.21.0",
    "robotskirt": "*",
    "showdown": "*"
  },
  "scripts": {
    "//test": "node test",
    "//bench": "node test --bench",

    "test": "node_modules/.bin/_mocha --reporter spec --check-leaks -u exports spec/mdast.spec.js",

    "lint": "npm run lint-api && npm run lint-test && npm run lint-style",
    "lint-test": "node_modules/.bin/eslint spec/mdast.spec.js --env node --env mocha --rule 'quotes: [2, single]'",
    "lint-api": "node_modules/.bin/eslint index.js lib/mdast.js --env node --env browser --rule 'no-spaced-func: false, consistent-return: false, no-loop-func: false, quotes: [2, single]'",
    "lint-style": "node_modules/.bin/jscs index.js lib/mdast.js spec/mdast.spec.js --reporter=inline",

    "coverage": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -- spec/mdast.spec.js",
    "make": "npm run lint && npm run coverage"
  }
}
